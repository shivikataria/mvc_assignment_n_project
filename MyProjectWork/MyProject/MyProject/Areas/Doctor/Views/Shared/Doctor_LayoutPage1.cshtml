<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>

    <!-- Favicons -->
    <link href="~/Areas/Doctor/doctortemp/assets/img/favicon.png" rel="icon">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Areas/Doctor/doctortemp/assets/css/bootstrap.min.css">

    <!-- Fontawesome CSS -->
    <link rel="stylesheet" href="~/Areas/Doctor/doctortemp/assets/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/Areas/Doctor/doctortemp/assets/plugins/fontawesome/css/all.min.css">

    <!-- Main CSS -->
    <link rel="stylesheet" href="~/Areas/Doctor/doctortemp/assets/css/style.css">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="assets/js/html5shiv.min.js"></script>
        <script src="assets/js/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <!-- Main Wrapper -->
    <div class="main-wrapper">

        <!-- Header -->
        <header class="header">
            <nav class="navbar navbar-expand-lg header-nav">
                <div class="navbar-header">
                    <a id="mobile_btn" href="javascript:void(0);">
                        <span class="bar-icon">
                            <span></span>
                            <span></span>
                            <span></span>
                        </span>
                    </a>
                    <a href="/FirstPage/First/MainDashboard" class="navbar-brand logo">
                        <img src="~/Dimg/logo/img1.jpeg" class="img-fluid" alt="Logo">
                    </a>
                </div>
                <div class="main-menu-wrapper">
                    <div class="menu-header">
                        <a href="index-2.html" class="menu-logo">
                            <img src="~/Dimg/logo/img1.jpeg" class="img-fluid" alt="Logo">
                        </a>
                       
                    </div>
                </div>
            </nav>
        </header>
        <!-- /Header -->
       <div>
           @RenderBody()
       </div>
        <!-- Footer -->
        <footer class="footer">

            <!-- Footer Top -->
            <div class="footer-top">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-3 col-md-6">

                            <!-- Footer Widget -->
                            <div class="footer-widget footer-about">
                                <div class="footer-logo">
                                    <img src="~/Dimg/logo/img1.jpeg" alt="logo">
                                </div>
                            </div>
                            <!-- /Footer Widget -->

                        </div>

                        <div class="col-lg-3 col-md-6">

                            <!-- Footer Widget -->
                            <div class="footer-widget footer-menu">
                                <h2 class="footer-title">For Patients</h2>
                                <ul>
                                    <li><a href="login.html"><i class="fas fa-angle-double-right"></i> Login</a></li>
                                    <li><a href="register.html"><i class="fas fa-angle-double-right"></i> Register</a></li>
                                    <li><a href="patient-dashboard.html"><i class="fas fa-angle-double-right"></i> Patient Dashboard</a></li>
                                </ul>
                            </div>
                            <!-- /Footer Widget -->

                        </div>

                        <div class="col-lg-3 col-md-6">

                            <!-- Footer Widget -->
                            <div class="footer-widget footer-contact">
                                <h2 class="footer-title">Contact Us</h2>
                                <div class="footer-contact-info">
                                    <div class="footer-address">
                                        <span><i class="fas fa-map-marker-alt"></i></span>
                                        <p> Rajasthan , INDIA </p>
                                    </div>
                                    <p>
                                        <i class="fas fa-phone-alt"></i>
                                       9876543210
                                    </p>
                                   
                                </div>
                            </div>
                            <!-- /Footer Widget -->

                        </div>

                    </div>
                </div>
            </div>
            <!-- /Footer Top -->
          

        </footer>
        <!-- /Footer -->

    </div>
    <!-- /Main Wrapper -->
    <!-- jQuery -->
    <script src="~/Areas/Doctor/doctortemp/assets/js/jquery.min.js"></script>

    <!-- Bootstrap Core JS -->
    <script src="~/Areas/Doctor/doctortemp/assets/js/popper.min.js"></script>
    <script src="~/Areas/Doctor/doctortemp/assets/js/bootstrap.min.js"></script>

    <!-- Sticky Sidebar JS -->
    <script src="~/Areas/Doctor/doctortemp/assets/plugins/theia-sticky-sidebar/ResizeSensor.js"></script>
    <script src="~/Areas/Doctor/doctortemp/assets/plugins/theia-sticky-sidebar/theia-sticky-sidebar.js"></script>

    <!-- Circle Progress JS -->
    <script src="~/Areas/Doctor/doctortemp/assets/js/circle-progress.min.js"></script>

    <!-- Custom JS -->
    <script src="~/Areas/Doctor/doctortemp/assets/js/script.js"></script>

</body>
</html>

<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/Areas/AdminA/doctortemp/admin/assets/img/favicon.png">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/Areas/AdminA/doctortemp/admin/assets/css/bootstrap.min.css">

    <!-- Fontawesome CSS -->
    <link rel="stylesheet" href="~/Areas/AdminA/doctortemp/admin/assets/css/font-awesome.min.css">

    <!-- Feathericon CSS -->
    <link rel="stylesheet" href="~/Areas/AdminA/doctortemp/admin/assets/css/feathericon.min.css">

    <link rel="stylesheet" href="~/Areas/AdminA/doctortemp/admin/assets/plugins/morris/morris.css">

    <!-- Main CSS -->
    <link rel="stylesheet" href="~/Areas/AdminA/doctortemp/admin/assets/css/style.css">

    <!--[if lt IE 9]>
        <script src="assets/js/html5shiv.min.js"></script>
        <script src="assets/js/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <!-- Main Wrapper -->
    <div class="main-wrapper">

        <!-- Header -->
        <div class="header">

            <!-- Logo -->
            <div class="header-left">
                <a href="/AdminA/Admin/AdminDashboard" class="logo">
                    <img src="~/Dimg/logo/img1.jpeg" alt="Logo">
                </a>
                <a href="/AdminA/Admin/AdminDashboard" class="logo logo-small">
                    <img src="~/Dimg/logo/img1.jpeg" alt="Logo" width="36" height="38">
                </a>
            </div>
            <!-- /Logo -->

            <div class="top-nav-search">
                <form>
                    <input type="text" class="form-control" placeholder="Search here">
                    <button class="btn" type="submit"><i class="fa fa-search"></i></button>
                </form>
            </div>

            <!-- Mobile Menu Toggle -->
            <a class="mobile_btn" id="mobile_btn">
                <i class="fa fa-bars"></i>
                9876543210
            </a>
            <!-- /Mobile Menu Toggle -->

        </div>
        <!-- /Header -->
        <!-- Sidebar -->
        <div class="sidebar" id="sidebar">
            <div class="sidebar-inner slimscroll">
                <div id="sidebar-menu" class="sidebar-menu">
                    <ul>
                        <li class="menu-title">
                            <span>Main</span>
                        </li>
                        <li class="active">
                            <a href="/AdminA/Admin/AdminDashboard"><i class="fe fe-home"></i> <span>Dashboard</span></a>
                        </li>
                        <li>
                            <a href="/AdminA/Admin/AppointmentList"><i class="fe fe-layout"></i> <span>Appointments</span></a>
                        </li>

                        <li>
                            <a href="/AdminA/Admin/DoctorsList"><i class="fe fe-user-plus"></i> <span>Doctors</span></a>
                        </li>
                        <li>
                            <a href="/AdminA/Admin/HospitalsList"><i class="fe fe-user-plus"></i> <span>Hospitals</span></a>
                        </li>

                        <li>
                            <a href="/AdminA/Admin/PatientList"><i class="fe fe-user"></i> <span>Patients</span></a>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
        <!-- /Sidebar -->
        <!-- Page Wrapper -->
       
            <div>
                @RenderBody()
            </div>

        <!-- /Page Wrapper -->

    </div>
    <!-- /Main Wrapper -->
    <!-- jQuery -->
    <script src="~/Areas/AdminA/doctortemp/admin/assets/js/jquery-3.2.1.min.js"></script>

    <!-- Bootstrap Core JS -->
    <script src="~/Areas/AdminA/doctortemp/admin/assets/js/popper.min.js"></script>
    <script src="~/Areas/AdminA/doctortemp/admin/assets/js/bootstrap.min.js"></script>

    <!-- Slimscroll JS -->
    <script src="~/Areas/AdminA/doctortemp/admin/assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <script src="~/Areas/AdminA/doctortemp/admin/assets/plugins/raphael/raphael.min.js"></script>
    <script src="~/Areas/AdminA/doctortemp/admin/assets/plugins/morris/morris.min.js"></script>
    <script src="~/Areas/AdminA/doctortemp/admin/assets/js/chart.morris.js"></script>

    <!-- Custom JS -->
    <script src="~/Areas/AdminA/doctortemp/admin/assets/js/script.js"></script>

</body>
</html>
